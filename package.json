{
    "name": "my-app",
    "version": "1.0.0",
    "description": "my-app",
    "main": "Main.js",
    "scripts": {
        "dev": "webpack-dev-server --watch --config webpack/build.config.js --open --mode development",
        "build": "webpack --config webpack/production.config.js",
